    background-color:#fff;
}
.top.topsearcharea{
    width:100%;
    float:left;
    margin-top:0 !important;
    padding-top:20rpx;
    padding-bottom:20rpx;
    background-color:#f3f3f3;
}
.top .focustopsearch{
    width:80%;
    margin:0;
    float:left;
    margin-left:20rpx;
    border:0;
    background-color:#e6e6e6;
    border-radius:8rpx;
}
.top .cancel{
    float:right;
    width:16%;
    text-align:center;
    height:64rpx;

}
.top .cancel text{
    font-size:28rpx;
}
.focustopsearch .specinput{
    float:left;
    margin-left:30rpx;
}
.focustopsearch .specinput input{
    width:65vw;
    margin-top:6rpx;
}
.focustopsearch .specimage{
    float:left;
    margin-left:20rpx;
    height:64rpx;

}
.focustopsearch .specimage image{
    width:40rpx;
    height:40rpx;
    margin-top:12rpx;
}
.showcontent{
    width:96%;
    height: 105rpx;
    margin:20rpx auto 0 auto;
    background-color:#fff;
    border-bottom:2rpx solid #eee;
}
.showcontent text{
    margin-left:20rpx;
    display: block;
    font-size:28rpx;
}
.showcontent text:last-child{
    margin-top:10rpx;
    color:#ccc;
}